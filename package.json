    "start:server": "nodemon server/index.js",
    "build": "react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "pretty": "prettier --write \"*/**/*.{js,jsx,json,ts}\"",
    "lint": "eslint .",
    "db:create": "sequelize-cli db:create",
